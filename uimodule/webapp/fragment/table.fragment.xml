<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog id="tableDialog" title="Enter the details" close="onDialogClose">
			<!-- <List headerText="Input"> -->
        <List id="list">
		<InputListItem>
			<ObjectStatus id="jobId" title="JobId" text="JB001" class="sapUiTinyMargin"/>
		</InputListItem>
		<InputListItem>
			<Input
				id="fileName"
                placeholder="FileName"
				type="text" />
		</InputListItem>
		<InputListItem>
			<Input
				id="docType"
                placeholder="DocumentType"
				type="text" />
		</InputListItem>
		<InputListItem>
			<Input
                placeholder="CreatedOn"
				type="text" />
		</InputListItem>
		<InputListItem>
			<Input
                placeholder="Status"
				type="text" />
		</InputListItem>
		<InputListItem>
			<Input
                placeholder="FinishedOn"
				type="text" />
		</InputListItem>
		<InputListItem>
			<Input
                placeholder="ClientId"
				type="text" />
		</InputListItem>
		<InputListItem>
			<Input
                placeholder="Sender"
				type="text" />
		</InputListItem>
		<InputListItem>
			<Input
                placeholder="Flag"
				type="text" />
		</InputListItem>
	</List>
    <OverflowToolbar>
    <Button text="Save" type="Accept" press="onSave"/>
    <ToolbarSpacer/>
    <Button text="Close" press="onDialogClose"/>
    </OverflowToolbar>
	</Dialog>
</core:FragmentDefinition>